{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "/etc/sing-box/ui",
      "secret": "",
      "external_ui_download_url": "https://gh-proxy.com/https://github.com/Zephyruso/zashboard/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "🎯 全球直连",
      "default_mode": "global"
    },
    "cache_file": {
      "enabled": true,
      "path": "/etc/sing-box/cache.db",
      "store_fakeip": true
    }
  },
  "dns": {
    "servers": [
      {
        "address": "fakeip",
        "fakeip": {
          "enabled": true,
          "inet4_range": "8.0.0.0/8"
        }
      },
      {
        "address": "192.168.1.1:5304",
        "detour": "dns-out"
      }
    ],
    "final": "fakeip"
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "address": [
        "172.19.0.1/30"
      ],
      "mtu": 1500,
      "stack": "mixed",
      "auto_route": true,
      "strict_route": true,
      "sniff": true,
      "sniff_override_destination": true
    },
    {
      "type": "dns",
      "tag": "dns-in",
      "listen": "0.0.0.0",
      "listen_port": "1053",
      "sniff": true
    },
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "0.0.0.0",
      "listen_port": "1080",
      "sniff": true,
      "set_system_proxy": false
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "dns-out"
    },
    {
      "type": "selector",
      "tag": "auto-proxy",
      "outbounds": [
        "♻️ 自动选择"
      ]
    },
    {
      "tag": "♻️ 自动选择",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "exclude",
          "keywords": [
            "网站|地址|剩余|过期|时间|有效|邀请|一元|yiyuanjichang"
          ]
        }
      ],
      "url": "http://cp.cloudflare.com/generate_204",
      "interval": "5m",
      "tolerance": 50
    },
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {
    "rules": [
      {
        "inbound": [
          "dns-in"
        ],
        "outbound": "dns-out"
      },
      {
        "inbound": [
          "mixed-in"
        ],
        "outbound": "auto-proxy"
      },
      {
        "ip_cidr": [
          "8.0.0.0/8"
        ],
        "outbound": "auto-proxy"
      }
    ],
    "final": "auto-proxy",
    "auto_detect_interface": true
  }
}
